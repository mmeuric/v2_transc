{
  "title": "Services Health (Blackbox)",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Probe Success",
      "targets": [
        {
          "expr": "avg by (instance) (probe_success{job=\"blackbox_http\"})"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Latency (ms)",
      "targets": [
        {
          "expr": "avg by (instance) (probe_duration_seconds{job=\"blackbox_http\"}) * 1000"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 6
      }
    },
    {
      "type": "table",
      "title": "HTTP Status Code",
      "targets": [
        {
          "expr": "max by (instance) (probe_http_status_code{job=\"blackbox_http\"})"
        }
      ],
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 14
      }
    }
  ],
  "schemaVersion": 39,
  "version": 1
}